{
    "name": "electron-quick-start",
    "version": "1.0.0",
    "description": "A minimal Electron application",
    "main": "final-app/main.js",
    "scripts": {
        "postinstall": "install-app-deps",
        "build": "gulp build",
        "prerelease": "gulp build --env=production",
        "release": "build",
        "start": "gulp start"
    },
    "build": {
        "appId": "com.example.electron-template",
        "files": [
            "final-app/**/*",
            "node_modules/**/*",
            "package.json"
        ],
        "publish": null
    },
    "repository": "https://github.com/electron/electron-quick-start",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "GitHub",
    "license": "CC0-1.0",
    "dependencies": {
        "fs-jetpack": "^1.0.0"
    },
    "devDependencies": {
        "electron": "^1.6.8",
        "electron-builder": "^17.1.1",
        "gulp": "^3.9.1",
        "gulp-batch": "^1.0.5",
        "gulp-concat": "^2.6.1",
        "gulp-less": "^3.3.0",
        "gulp-plumber": "^1.1.0",
        "gulp-util": "^3.0.8",
        "gulp-watch": "^4.3.11",
        "minimist": "^1.2.0",
        "rollup": "^0.41.4",
        "rollup-plugin-istanbul": "^1.1.0",
        "source-map-support": "^0.4.11"
    }
}